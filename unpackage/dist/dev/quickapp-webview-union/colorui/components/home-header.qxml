<view><view class="cu-custom" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="{{['cu-bar bg-red search fixed justify-start flex',bgImage!=''?'none-bg text-white bg-img':'',bgColor]}}" style="{{(style)}}"><block qa:if="{{isBack}}"><view data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text><slot name="backText"></slot></view></block><block qa:if="{{logo}}"><view class="flex align-center logo_box padding-left-sm"><image class="logo" style="width:200rpx;height:48rpx;" src="{{logo}}"></image></view></block><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="search-form radius" bindtap="__e"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="搜索站内资讯" confirm-type="search" data-event-opts="{{[['focus',[['InputFocus',['$event']]]],['blur',[['InputBlur',['$event']]]],['confirm',[['confirm',['$event']]]],['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" bindfocus="__e" bindblur="__e" bindconfirm="__e" bindinput="__e"/></view></view></view></view>